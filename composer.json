{
  "name": "bronos/php-sql-schema",
  "description": "PHP SQL schema",
  "keywords": [
    "php",
    "db",
    "sql",
    "schema",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Bronzov",
      "email": "oleg.bronzov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "BronOS\\PhpSqlSchema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BronOS\\PhpSqlSchema\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-clover=coverage.xml"
  }
}
